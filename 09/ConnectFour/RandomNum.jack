class RandomNum
{

 static int seed; 

 	constructor RandomNum new(int theSeed)
	{
		let seed=theSeed;
		return this; 
	}

	function int randomGen()
	{	
		let seed=seed+54; 
		return seed; 
	}

	function int rand(int a )
	{
		var int i;
		let i=0;

		while (i<5)
		{
			let seed = seed*2+3 ;
			let i=i+1; 
		}
		return seed; 
	}
	
	
}